      "source": [
        "# Store the image, patient ID number, and the class/target (whether the image is +/- for IDC) together in a single DataFrame\n",
        "\n",
        "# Initialize empty lists to store data\n",
        "patient_ids = []\n",
        "paths = []\n",
        "targets = []\n",
        "\n",
        "# Iterate through the folders\n",
        "for patient_id in os.listdir(base_path):\n",
        "    patient_path = os.path.join(base_path, patient_id)\n",
        "    if os.path.isdir(patient_path):\n",
        "        for c in [0, 1]:\n",
        "            class_path = os.path.join(patient_path, str(c))\n",
        "            if os.path.isdir(class_path):\n",
        "                for file_name in os.listdir(class_path):\n",
        "                    file_path = os.path.join(class_path, file_name)\n",
        "                    patient_ids.append(patient_id)\n",
        "                    paths.append(file_path)\n",
        "                    targets.append(c)\n",
        "\n",
        "# Create DataFrame from the lists\n",
        "df = pd.DataFrame({\n",
        "    'patient_id': patient_ids,\n",
        "    'path': paths,\n",
        "    'target': targets\n",
        "})\n",
        "\n",
        "# Print first few rows to check everything worked as expected\n",
        "print(df.head())"
      ]
    },
