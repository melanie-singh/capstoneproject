        "# View some images from the dataset\n",
        "\n",
        "def plot_idc_images(image_indices, title):\n",
        "    # Set figure dimensions\n",
        "    fig, ax = plt.subplots(2, 5, figsize=(15, 8))\n",
        "\n",
        "    # Loop through first 10 indices\n",
        "    for i in range(10):\n",
        "        # Get the index for the current image\n",
        "        idx = image_indices[i]\n",
        "\n",
        "        # Load the image using the file path stored in the DataFrame\n",
        "        image = mpimg.imread(df.loc[idx, 'path'])\n",
        "\n",
        "        # Display the image on the appropriate subplot\n",
        "        ax[i // 5, i % 5].imshow(image)\n",
        "        ax[i // 5, i % 5].grid(False) # turn off grid lines\n",
        "        ax[i // 5, i % 5].axis('off') # turn off axis labels and ticks\n",
        "\n",
        "    # Set title\n",
        "    plt.suptitle(title, fontsize=20)\n",
        "    plt.show()\n",
        "\n",
        "# Plot IDC-positive images\n",
        "plot_idc_images(IDC_positive[:10], 'IDC Positive Images')\n",
        "\n",
        "# Plot IDC-negative images\n",
        "plot_idc_images(IDC_negative[:10], 'IDC Negative Images')"
