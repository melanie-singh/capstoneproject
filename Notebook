        "  plt.ylabel('Actual', fontsize=14)\n",
        "  plt.title('SVM Confusion Matrix', fontsize=16)\n",
        "  plt.show()\n",
        "\n",
        "# Implement SVM model\n",
        "runSVM(X_train, y_train, X_test, y_test)"
