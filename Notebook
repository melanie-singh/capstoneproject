        "\n",
        "def runSVM(X_train, y_train, X_test, y_test):\n",
        "\n",
        "  # Modelling + cross-validation\n",
        "  SVM = SVC(probability=False)\n",
        "  SVM.fit(X_train, y_train)\n",
        "  kfold = KFold(n_splits=10, shuffle=True, random_state=42)\n",
        "  cv_accuracy = cross_val_score(SVM, X_train, y_train, cv=kfold, scoring='accuracy')\n",
        "\n",
        "  # Evaluate model\n",
        "  y_pred = SVM.predict(X_test)\n",
        "  test_accuracy = accuracy_score(y_test, y_pred)\n",
        "  conf_matrix = confusion_matrix(y_test, y_pred)\n",
        "  class_report = classification_report(y_test, y_pred)\n",
        "\n",
        "  # Print evaluation metrics\n",
        "  print(f\"Cross-Validation Accuracy: {np.mean(cv_accuracy):.4f} ± {np.std(cv_accuracy):.4f}\")\n",
        "  print(f\"Test Accuracy: {test_accuracy:.4f}\")\n",
        "  print(\"Confusion Matrix:\\n\")\n",
        "  print(conf_matrix)\n",
        "  print() # for spacing\n",
        "  print(\"Classification Report:\\n\")\n",
        "  print(class_report)\n",
        "\n",
        "  # Visualise matrix with a heatmap\n",
        "  plt.figure(figsize=(8, 6))\n",
        "  sns.heatmap(conf_matrix, annot=True, square=True, fmt='.1f', cmap='RdPu', xticklabels=['Negative', 'Positive'], yticklabels=['Negative', 'Positive'])\n",
